{
	"name": "vega-dark-focus-theme",
	"displayName": "Vega",
	"description": "A programming focused theme.",
	"icon": "images/icon.png",
	"publisher": "RayConcepts",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"repository" : {
		"type": "git",
		"url": "https://github.com/ray-concepts/VSC-Themes/tree/main/Vega"
	},
	"scripts": {
		"build": "ts-node src/index.ts",
		"build:ext": "vsce package --no-yarn",
		"start": "nodemon",
		"fix": "prettier --write ./src/**/*.ts"
	},
	"nodemonConfig": {
		"ext": "ts",
		"exec": "ts-node src/index.ts"
	},
	"keywords": [
		"vega",
		"theme",
		"ray",
		"concepts",
		"rayconcepts",
		"contrast",
		"pastel",
		"dark",
		"neon"
	],
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Vega by Ray Concepts",
				"uiTheme": "vs-dark",
				"path": "./themes/vega-dark-focus-theme.json"
			}
		]
	},
	"dependencies": {
		"@types/node": "^17.0.22",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"nodemon": "^2.0.15",
		"ovsx": "^0.3.0",
		"prettier": "^2.6.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.2",
		"vsce": "^2.7.0"
	},
	"__metadata": {
		"publisherDisplayName": "Ray Concepts",
		"updated": true,
		"isPreReleaseVersion": false,
		"preRelease": false
	}
}
